    "@testing-library/user-event": "^13.5.0",
    "daisyui": "^2.51.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "theme-change": "^2.5.0",
    "typescript": "^4.9.5",
    "usehooks-ts": "^2.9.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.9",
